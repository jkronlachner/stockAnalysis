{
  "name": "stock-analysis",
  "description": "An easy way to check stocks and data for correlation",
  "version": "0.2.5",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.9.5",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@nivo/core": "^0.67.0",
    "@nivo/line": "^0.67.0",
    "@reduxjs/toolkit": "^1.4.0",
    "@storybook/addon-backgrounds": "^6.0.20",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@testing-library/user-event": "^12.2.2",
    "auto-updater": "^1.0.2",
    "axios": "^0.21.0",
    "dotenv": "^8.2.0",
    "electron-is-dev": "^1.2.0",
    "electron-log": "^4.3.0",
    "electron-updater": "^4.3.5",
    "jquery": "^3.5.1",
    "lodash": "^4.17.20",
    "nivo": "^0.31.0",
    "papaparse": "^5.3.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-alert": "^7.0.2",
    "react-alert-template-mui": "^1.0.7",
    "react-dom": "^17.0.1",
    "react-dropzone": "^11.2.4",
    "react-google-charts": "^3.0.15",
    "react-loading-skeleton": "^2.1.1",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "react-sage": "^0.1.3",
    "react-scripts": "4.0.1",
    "recharts": "^1.8.5",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "tree-kill": "^1.2.2",
    "web-vitals": "^0.2.4",
    "shelljs": "^0.8.4"
  },
  "homepage": "./",
  "main": "public/electron.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "electron-dev": "concurrently \"cross-env BROWSER=none npm start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron-only": "electron .",
    "preelectron-pack": "npm run-script build",
    "electron-pack": "electron-builder -w",
    "electron-pack-only-mac": "electron-builder -m",
    "electron-pack-only-win": "electron-builder -w"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.3",
    "electron": "^11.0.3",
    "electron-builder": "^22.9.1",
    "wait-on": "^5.2.0"
  },
  "author": {
    "name": "Julian Kronlachner",
    "email": "julian.kronlachner@lab73.at",
    "url": "https://lab73.at"
  },
  "build": {
    "appId": "com.lab73.stockAnalysis",
    "productName": "StockAnalysis",
    "copyright": "Copyright Â© 2021 ${author}",
    "mac": {
      "extendInfo": {
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        }
      },
      "category": "public.app-category.utilities",
      "icon": "./build/icon.icns",
      "publish": {
        "provider": "github",
        "vPrefixedTagName": "false"
      }
    },
    "win": {
      "requestedExecutionLevel": "requireAdministrator",
      "target": "nsis",
      "publish": {
        "provider": "github",
        "vPrefixedTagName": "false"
      }
    },
    "nsis": {
      "artifactName": "${productName}-Setup-${version}.${ext}",
      "oneClick": true,
      "perMachine": false,
      "deleteAppDataOnUninstall": true,
      "allowToChangeInstallationDirectory": false,
      "runAfterFinish": true,
      "differentialPackage": true
    },
    "icon": "./build/icon.png",
    "files": [
      "build/**/*"
    ],
    "extraFiles": [
      "output/**/*"
    ],
    "directories": {
      "output": "release",
      "buildResources": "output"
    }
  }
}
